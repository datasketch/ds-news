{
  "name": "ds-news",
  "version": "1.0.0",
  "description": "Datasketch News site",
  "main": "index.js",
  "scripts": {
    "start": "DEBUG=ds-news* node index.js",
    "build": "postcss client/css/*.css -d public/styles",
    "watch": "postcss client/css/*.css -d public/styles --watch --verbose",
    "dev": "concurrently \"npm run watch\" \"DEBUG=ds-news* nodemon index.js --ignore public/ --ignore views/\""
  },
  "author": "David Daza <03dazal@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "i18n": "^0.8.3",
    "moment": "^2.24.0",
    "mongoose": "^5.6.4",
    "morgan": "^1.9.1",
    "postcss-cli": "^6.1.3",
    "pug": "^2.0.3",
    "tailwindcss": "^1.0.5"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "concurrently": "^4.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.19.1"
  },
  "engines": {
    "node": "v10.16.0",
    "npm": "6.9.0"
  }
}
